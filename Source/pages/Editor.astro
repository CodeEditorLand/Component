---
(await import("dotenv")).config();

const { API, Socket } = (
	(await import("@codeeditorland/wind/Target/Library/Environment.js"))
		._Function as Exclude<typeof _Function, boolean>
).parse(process.env);

import type { _Function } from "@codeeditorland/wind/Target/Library/Environment.js";

export const { default: Layout } = await import("@Layout/Base.astro");

export const { default: Page } = await import(
	"@codeeditorland/wind/Target/Element/Page/Editor.js"
);
---

<!--
	TODO: Implement font changes in layout 
	<Layout Font={["/Font/AlbertSans/800.css"]}>
-->
<Layout>
	<Page client:only="solid-js" {API} {Socket} />
</Layout>

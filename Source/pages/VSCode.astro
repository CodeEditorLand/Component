---
import Layout from "@Layout/Base.astro";
---

<Layout>
	<Fragment slot="scripts">
		<script>
			performance.mark("code/didStartRenderer");
		</script>
	</Fragment>

	<script>
		const baseUrl = new URL(
			"{{WORKBENCH_WEB_BASE_URL}}",
			window.location.origin,
		).toString();

		// @ts-expect-error
		globalThis._VSCODE_FILE_ROOT = baseUrl + "/out/";
	</script>
	<script>
		import "@codeeditorland/vanilla/Target/CodeEditorLand/Editor/vs/workbench/browser/workbench.js";
	</script>
</Layout>
